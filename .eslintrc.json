{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
    "globals": {
        "ko": true,
        "showModal": true,
        "LoginViewModel": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "no-extra-boolean-cast": "warn",
        "no-iterator": "warn",
        "no-invalid-regexp": "warn",
        "no-obj-calls": "warn",
        "no-sparse-arrays": "warn",
        "no-unreachable": "warn",
        "no-dupe-keys": "warn",
        "no-multi-str": "warn",
        "no-extend-native": "warn",
        "guard-for-in": "warn",
        "no-func-assign": "warn",
        "no-extra-semi": "warn",
        "camelcase": "warn",
        "no-mixed-spaces-and-tabs": "warn",
        "no-undef": "warn",
        "semi": "warn",
        "no-empty-character-class": "warn",
        "complexity": "warn",
        "no-dupe-class-members": "warn",
        "no-debugger": "warn",
        "block-scoped-var": "warn",
        "no-loop-func": "warn",
        "no-use-before-define": "warn",
        "no-console": "warn",
        "require-yield": "warn",
        "no-redeclare": "warn",
        "no-undefined": "warn",
        "use-isnan": "warn",
        "no-control-regex": "warn",
        "no-const-assign": "warn",
        "no-new": "warn",
        "new-cap": "warn",
        "no-irregular-whitespace": "warn",
        "object-shorthand": "warn",
        "no-ex-assign": "warn",
        "wrap-iife": "warn",
        "arrow-parens": "warn",
        "no-constant-condition": "warn",
        "no-octal": "warn",
        "no-dupe-args": "warn",
        "quotes": [ "warn", "single", { "avoidEscape": true }],
        "no-fallthrough": "warn",
        "no-delete-var": "warn",
        "no-caller": "warn",
        "no-cond-assign": "warn",
        "no-this-before-super": "warn",
        "no-negated-in-lhs": "warn",
        "no-inner-declarations": "warn",
        "eqeqeq": "warn",
        "curly": "warn",
        "no-eval": "warn",
        "arrow-spacing": "warn",
        "no-empty": "warn",
        "generator-star-spacing": "warn",
        "no-duplicate-case": "warn",
        "valid-typeof": "warn",
        "no-regex-spaces": "warn",
        "no-class-assign": "warn"
    }
}